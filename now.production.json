{
  "version": 2,
  "name": "twitch-token-bridge",
  "scope": "maximumsheep",
  "alias": "twitch-token-bridge.now.sh",
  "builds": [
    { "src": "src/lambda.js", "use": "@now/node" }
  ],
  "env": {
    "TWITCH_CLIENT_ID": "ggjwdwfc9q3itbql59yixzr2xy3yty",
    "TWITCH_CLIENT_SECRET": "@twitch-token-bridge-twitch-client-secret-production",
    "TWITCH_CLIENT_REDIRECT_URI": "https://twitch-token-bridge.now.sh/callback",
    "MONGO_URI": "@twitch-token-bridge-mongo-uri-production",
    "MONGO_DB": "twitch-token-bridge-production",
    "MAIN_CLIENT_SECRET": "@twitch-token-bridge-main-client-secret-production"
  },
  "routes": [
    { "src": "/.*", "dest": "src/lambda.js" }
  ]
}