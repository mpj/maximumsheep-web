{
  "version": 2,
  "name": "twitch-token-bridge-staging",
  "scope": "maximumsheep",
  "alias": "twitch-token-bridge-staging.now.sh",
  "builds": [
    { "src": "lambda.js", "use": "@now/node" }
  ],
  "env": {
    "TWITCH_CLIENT_ID": "uekwnk316lym0mxfc1egfglkkeqqvj",
